<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.gdin.netcentermanm.activity.ManagerListActivity">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#66BB6A">
        <ImageView
            android:id="@+id/img_return"
            android:layout_width="50dp"
            android:layout_height="match_parent"
            android:src="@drawable/ic_arrow_back_white_18dp"
            android:padding="10dp"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="管理员列表"
            android:layout_centerHorizontal="true"
            android:gravity="center"
            android:paddingLeft="16dp"
            android:paddingRight="16dp"
            android:textSize="17sp"
            android:textColor="#ffffff" />
    </RelativeLayout>


    <ListView
        android:id="@+id/lv_manager"
        android:layout_width="wrap_content"
        android:layout_height="300dp" />

    <TextView
        android:id="@+id/tv_add_manager"
        android:layout_width="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_height="40dp"
        android:layout_gravity="center"
        android:gravity="center"
        android:background="@drawable/button_selector_green_normal"
        android:textColor="#ffffff"
        android:text="添加管理员" />
</LinearLayout>
